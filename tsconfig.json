{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "outDir": "./dist/",
    "esModuleInterop": true,
    // "noImplicitAny": true,
    "allowJs": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true
    // "strict": true
  },
  "ts-node": {
    "compilerOptions": {
      "module": "CommonJS",
      "typeRoots": ["./src/alexandr/@types", "./node_modules/@types"]
    }
  }
}
